{
    "name": "copyswapper",
    "displayName": "copyswapper",
    "description": "Paste over selected text while copying what what previously there",
    "version": "1.0.3",
    "publisher": "berickson324",
    "repository": {
        "type": "git",
        "url": "https://github.com/BradErickson/CopySwapVsCodeExtension.git"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.copySwapper"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.copySwapper",
                "title": "Copy Swap"
            }
        ],
        "keybindings": [
            {
                "command": "extension.copySwapper",
                "key": "shift+ctrl+v",
                "mac": "shift+cmd+v"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "clipboardy": "^1.2.3"
    }
}
